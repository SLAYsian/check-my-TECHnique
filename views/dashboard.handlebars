<h2 class="welcome-name"> Welcome, {{name}}!</h2>
<div class="display-flex flex-column">
  {{#each userBlogPosts}}
      <div class="card flex-wrap">
      <div class="card-header">
        <h3><a href="/blogPost">{{this.title}}</a></h3>
        <p>{{format_date this.date_created}}</p>
      </div>
    </div>
  {{/each}}
  <button class="btn-styled"><a href="/blogpost">+ New Post</a></button>
</div>